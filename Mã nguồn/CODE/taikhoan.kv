MDScreen:
    name:"taikhoan"
    MDFloatLayout:
        md_bg_color: 1,1,1,1
        MDIconButton:
            icon:"arrow-left"
            pos_hint:{"center_y":.95}
            user_font_size:"30sp"
            theme_text_color: "Custom"
            text_color:rgba(26,24,58,255)
            on_release:
                root.manager.transition.direction="right"
                root.manager.current="login"
        MDLabel:
            text: "Giao diện tài khoản"
            font_size:"26sp"
            pos_hint:{"center_y":.85}
            halign:"center"
            color: rgba(0,0,59,255)
        MDLabel:
            text: "Cảm ơn bạn đã sử dụng ứng dụng của tôi"
            font_size:"18sp"
            pos_hint:{"center_y":.79}
            halign:"center"
            color: rgba(135,135,135,255)
        Button:
            text:"Đăng xuất"
            size_hint:0.3,0.065
            pos_hint:{"center_x":.5,"center_y":.34}
            background_clor:0,0,0,0
            on_release:
                root.manager.transition.direction="left"
                app.dangxuat()
                root.manager.current="main"
            canvas.before:
                Color:
                    rgb:rgba(52,0,231,255)
                RoundedRectangle:
                    size:self.size
                    pos:self.pos
                    radius:[5]